{
  "manifest_version": 2,
  "name": "RTP Midi Sample App",
  "version": "0.0.1",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    "storage",
    "experimental",
    {
      "socket": [
        "udp-send-to::*",
        "udp-bind::*"
      ]
    }
  ]
}
